package jpa.seleccion.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;

import jpa.seleccion.model.Seleccionados;
import jpa.seleccion.repository.SeleccionadosRepository;

public class SeleccionadosService {

	@Autowired
	private SeleccionadosRepository repo;
	
	public List<Seleccionados> listAll() {
        return repo.findAll();
    }
}
